<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cinemaprojectfx.RegisterClientController">
   <children>
      <Label layoutX="150.0" layoutY="29.0" text="Rejestracja">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="36.0" layoutY="98.0" text="Nazwa użytkownika:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="35.0" layoutY="118.0" prefHeight="25.0" prefWidth="161.0" />
      <Label layoutX="35.0" layoutY="155.0" text="Hasło:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="registerButton" layoutX="243.0" layoutY="327.0" mnemonicParsing="false" onAction="#onRegisterButtonClick" prefHeight="25.0" prefWidth="100.0" text="Zarejestruj" />
      <Button layoutX="35.0" layoutY="327.0" mnemonicParsing="false" onAction="#backMenu" prefHeight="25.0" prefWidth="100.0" text="Wstecz" />
      <Label layoutX="36.0" layoutY="213.0" text="Adres email:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="emailTextField" layoutX="36.0" layoutY="233.0" prefHeight="25.0" prefWidth="161.0" />
      <PasswordField fx:id="passwordField" layoutX="38.0" layoutY="175.0" prefHeight="25.0" prefWidth="161.0" />
   </children>
</AnchorPane>
